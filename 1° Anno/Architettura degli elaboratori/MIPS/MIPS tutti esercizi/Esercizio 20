# Da un bussolotto estarre 3 palline numerate e stampare su videoterminale il loro valore. Le palline sono numerate da 0 a 90 e non sono ammesse ripetizioni.

.text
.globl main

main:
	# generazione primo numero
	li $v0, 42	# codice sycall per genrazione di numero casuale
	li $a1, 91	# massimo valore ottenibile
	syscall	
	move $t0, $a0	# sposto valore nel registro t0
	move $a0, $t0
	li $v0, 1
	syscall
	
	# generazione secondo numero
	li $v0, 42	# codice sycall per genrazione di numero casuale
	li $a1, 91	# massimo valore ottenibile
	syscall	
	move $t1, $a0	# sposto valore nel registro t0
	move $a0, $t1
	li $v0, 1
	syscall
	
	# generazione terzo numero
	li $v0, 42	# codice sycall per genrazione di numero casuale
	li $a1, 91	# massimo valore ottenibile
	syscall	
	move $t2, $a0	# sposto valore nel registro t0
	move $a0, $t2
	li $v0, 1
	syscall
	
	li $v0,10
	syscall
	
	