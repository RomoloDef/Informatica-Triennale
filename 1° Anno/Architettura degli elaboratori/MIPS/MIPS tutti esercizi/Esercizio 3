# Scrivere un programma che converta un valore da scala Celsius a scala Fahrenheit. Riportare il risultato in $t0

.text
.globl main

main:
	lw $t1, gradoC
	li $t2, 9
	li $t3, 5
	li $t4, 32
	# formula per convertire da celsius a fahrenheit = (gradi * 9/5) + 32
	div $t5, $t2, $t3	# 9/5
	mulo $t0, $t1, $t5	# gradi * 9/5
	add $t0, $t0, $t4	# RISULTATO + 32
	
	li $v0,10
	syscall
	
.data
gradoC: .word 38